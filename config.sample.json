{
  "name": "mpcorbit server",
  "port": 8111,
  "servers": [
    {
      "name": "Playwright",
      "route": "playwright",
      "command": "npx",
      "args": ["-y", "@automatalabs/mcp-server-playwright"],
      "env": {
        "SAMPLE_API_KEY": "your_api_key_here"
      }
    },
    {
      "name": "Sequential Thinking",
      "route": "sequential-thinking",
      "command": "npx",
      "args": ["-y", "@modelcontextprotocol/server-sequential-thinking"]
    },
    {
      "name": "Obsidian",
      "route": "obsidian",
      "command": "npx",
      "mountPoint": "/data",
      "args": ["-y", "mcp-obsidian", "data"]
    }
  ]
}
